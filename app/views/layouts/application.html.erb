<!DOCTYPE html>
<html>
<head>
  <title>Workoutlog</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'layouts/header' %>
  <div id="wrap">
    <div class="container">
      <%= render 'layouts/notice' %>
      <%= yield %>
    </div>
    <div id="push"></div>
  </div>
  <%= render 'layouts/footer' %>
</body>
</html>
